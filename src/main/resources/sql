use service_station;

create table service_station.mechanic
(
    id int auto_increment primary key ,
    experience_mechanic varchar(50),
    name_mechanic varchar(50),
    surname_mechanic varchar(50)
);

create table service_station.mechanic_details
(
    mechanic_id int  ,
    experience_mechanic varchar(50),
    salary_mechanic varchar(50)
);

create table service_station.orders
(
    id int auto_increment primary key,
    name_orders varchar(50)
);
